generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model FieldModel {
    id String @id;
    name String;
    location String;
    extentSize String;
    image1  Bytes;
    image2  Bytes;
}  

model CropModel {
  id String @id;
  name String;
  scientificName String;
  category String;
  season String;
  fieldId String;
  image Bytes;
}

model EquipmentModel {
  id String @id;
  name String;
  type String;
  count String;
  status String;
  staffId String;
  fieldId String;  
}