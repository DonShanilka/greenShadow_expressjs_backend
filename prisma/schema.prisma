generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model FieldModel {
    id String @id;
    name String;
    location String;
    extentSize String;
    image1  Bytes;
    image2  Bytes;
}  

model CropModel {
  id String @id;
  name String;
  scientificName String;
  category String;
  season String;
  fieldId String;
  image Bytes;
}

model EquipmentModel {
  id String @id;
  name String;
  type String;
  count Int;
  status String;
  staffId String;
  fieldId String;  
}

model VehicalModel {
  id String @id;
    lPlate String;
    category String;
    fuleType String;
    status String;
    remarks String;
    staffId String;  
}

model StaffModel {
  id String @id;
  firstName String;
  lastName String;
  designation String;
  gender String;
  joinedDate Date;
  dateOfBirth Date;
  mobile Int;
  email  String @unique;
  role String;
  address1 String;
  address2 String;
  address3 String;
  address4 String;
  address5 String;
  fieldId String;  
}